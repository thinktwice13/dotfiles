{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.formatOnSave": true,
  "editor.matchBrackets": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.preferredLightColorTheme": "Light+ Tweaked",
  "prettier.singleQuote": true,
  "editor.cursorBlinking": "smooth",
  "window.autoDetectColorScheme": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[javascriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },

  // "editor.codeActionsOnSave": null,

  "editor.folding": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.lineNumbers": "off",
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "none",
  "explorer.openEditors.visible": 0,
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "workbench.iconTheme": null,
  "workbench.editor.showIcons": false,
  // "workbench.statusBar.visible": false,
  "javascript.validate.enable": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.parameterHints.enabled": true,
  "editor.fontLigatures": false,
  "editor.fontWeight": "500",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "jupyter.interactiveWindowMode": "perFile",
  "go.toolsManagement.autoUpdate": true,
  "terminal.integrated.fontFamily": "sf mono, menlo",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.tabSize": 2,
  "editor.fontFamily": "sf mono, menlo, monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 14,
  "liveshare.featureSet": "stable",
  "vs-kubernetes": {
    "vscode-kubernetes.minikube-path.mac": "/Users/mario/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
  },
  "editor.fontSize": 15,
  "redhat.telemetry.enabled": false,
  "workbench.preferredHighContrastColorTheme": "GitHub Light",
  "diffEditor.renderSideBySide": false,
  "workbench.preferredDarkColorTheme": "Dark+ Tweaked",
  "workbench.colorTheme": "Dark+ Tweaked"
  // "editor.hover.enabled": false
}
